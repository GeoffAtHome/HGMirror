{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "brace-style": "off",
    "new-cap": ["error", {
      "capIsNewExceptions": ["Polymer"]
    }],
    "no-var": "off",
    "require-jsdoc": "off"
  },
  "globals": {
    "Polymer": true
  }
}